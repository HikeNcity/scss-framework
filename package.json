{
  "name": "scss-boilerplate",
  "version": "1.0.0",
  "description": "A SCSS boilerplate using npm scripts.",
  "keywords": [
    "scss",
    "html"
  ],
  "author": "Imran Khan",
  "maintainers": [
    {
      "name": "Imran Khan",
      "email": "imransilvake@gmail.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "browser-sync": "browser-sync start --server  --files '**/*.html, dist/css/main.min.css' --port 8080 --browser 'google chrome'",
    "compile": "./app/code/core/scripts/scss/compile",
    "start": "./app/code/core/scripts/scss/watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "browser-sync": "^2.18.13",
    "clean-css-cli": "^4.1.10",
    "del-cli": "^0.2.1",
    "gulp": "^3.9.1",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "postcss-cli": "^3.2.0",
    "stylelint": "^7.13.0",
    "stylelint-order": "^0.4.3"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "normalize.css": "^7.0.0"
  }
}
