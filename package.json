{
  "name": "scss-boilerplate",
  "version": "1.0.0",
  "description": "A SCSS boilerplate using npm scripts.",
  "keywords": [
    "scss",
    "html"
  ],
  "author": "Imran Khan",
  "maintainers": [
    {
      "name": "Imran Khan",
      "email": "imransilvake@gmail.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "browser-sync": "browser-sync start --server  --files '**/*.html, dist/css/main.min.css' --port 8080 --browser 'google chrome'",
    "compile": "./app/code/core/scripts/scss/compile",
    "start": "./app/code/core/scripts/scss/watch"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "browser-sync": "^2.24.4",
    "clean-css-cli": "^4.1.11",
    "del-cli": "^1.1.0",
    "gulp": "^3.9.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "postcss-cli": "^5.0.0",
    "stylelint": "^9.2.1",
    "stylelint-order": "^0.8.1"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "normalize.css": "^8.0.0"
  }
}
