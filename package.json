{
  "name": "scss-boilerplate",
  "version": "1.0.0",
  "description": "A SCSS boilerplate using npm scripts.",
  "main": "index.js",
  "scripts": {
    "browser-sync": "browser-sync start --server --files \"*.html, dist/css/main.min.css\" --port 8080 --browser \"google chrome\"",
    "scss-compile": "./src/core/scripts/scss/compile",
    "scss-watch": "./src/core/scripts/scss/watch"
  },
  "keywords": [
    "scss",
    "html"
  ],
  "author": "Imran Khan",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "browser-sync": "^2.18.8",
    "clean-css-cli": "^4.0.12",
    "del-cli": "^0.2.1",
    "gulp": "^3.9.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "postcss-cli": "^3.2.0",
    "stylelint": "^7.10.1",
    "stylelint-order": "^0.4.3"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0"
  }
}
